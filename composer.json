{
    "name": "phlexible/tinymce-bundle",
    "description": "phlexible tinymce bundle",
    "license": "proprietary",
	"type": "symfony-bundle",
	"keywords": ["tinymce", "editor", "rich text"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\TinymceBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.3.0",
        "phlexible/gui-bundle": "~1.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.brainbits.net/repo/private/",
            "options": {
                "ssl": {
                    "allow_self_signed": "true"
                }
            }
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
