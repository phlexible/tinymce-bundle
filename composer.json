{
    "name": "phlexible/tinymce-bundle",
    "description": "phlexible tinymce bundle",
    "license": "proprietary",
	"type": "symfony-bundle",
	"keywords": ["tinymce", "editor", "rich text"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\TinymceBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.3.0",
        "phlexible/phlexible": "dev-master",
        "jms/cg": "dev-master",
        "phansys/getid3": "v2.0.0-BETA1",

        "puli/repository": "1.0.0-beta",
        "puli/composer-plugin": "1.0.0-beta",
        "puli/repository-manager": "1.0.0-beta",
        "puli/discovery": "1.0.0-beta",
        "puli/cli": "1.0.0-beta",
        "puli/factory": "1.0.0-beta",
        "puli/twig-extension": "1.0.0-beta",
        "puli/symfony-bundle": "1.0.0-beta",
        "puli/symfony-bridge": "1.0.0-beta",
        "webmozart/json": "1.0.0-beta",
        "webmozart/glob": "1.0.0-beta",
        "webmozart/key-value-store": "1.0.0-beta"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.brainbits.net/phlexible-bundles/",
            "options": {
                "ssl": {
                    "allow_self_signed": "true"
                }
            }
        }
    ],
    "minimum-stability": "stable"
}
