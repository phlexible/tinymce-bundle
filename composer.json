{
    "name": "phlexible/tinymce-bundle",
    "description": "phlexible tinymce bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["tinymce", "editor", "rich text"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\TinymceBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phlexible/gui-bundle": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "symfony/config": "~2.8",
        "symfony/dependency-injection": "~2.8",
        "symfony/http-kernel": "~2.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
